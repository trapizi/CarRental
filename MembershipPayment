import java.util.Calendar;

public class MembershipPayment extends Payment {
    
    private String status;
    private String lastMatch;
    private Calendar nextExpiry;
    private int durationToExpiry;
    private int refundFlag;
    
    public String getStatus(){
        return status;
    }
    
    public void setStatus(String status){
        this.status = status;
    }
    
    public String getLastMatch(){
        return lastMatch;
    }
    
    public void setLastMatch(String lastMatch){
        this.lastMatch = lastMatch;
    }
        
    public Calendar getNextExpiry(){
        return nextExpiry;
    }
    
    public void setNextExpiry(Calendar nextExpiry){
        this.nextExpiry = nextExpiry;
    }
    
    public int getDurationToExpire(){
        return durationToExpiry;
    }
    
    public void setDurationToExpire(int durationtToExpiry){
        this.durationToExpiry = durationToExpiry;
    }
    
    public int getRefindFlag(){
        return refundFlag;
    }
    
    public void setRefundFlag(int refundFlag){
        this.refundFlag = refundFlag;
    }
}
